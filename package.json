{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev": "concurrently \"npm --workspace frontend run dev\" \"npm --workspace backend run dev\"",
    "start:frontend": "npm --workspace frontend run dev",
    "start:backend": "npm --workspace backend run dev"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
